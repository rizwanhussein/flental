<h1>Edit flat</h1>
<%= simple_form_for(@flat) do |f| %>
    <div class="form-inputs">
      <%= f.input :name %>
      <%= f.input :street_address %>
      <%= f.input :description %>
      <%= f.input :capacity, collection: 1..10 %>
      <%= f.input :photo, as: :file %>
      <%= f.button :submit %>
    </div>
<% end %>
